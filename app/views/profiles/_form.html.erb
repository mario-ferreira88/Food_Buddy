<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :phone_number %>
    <%= f.input :address %>
    <%= f.input :photo, as: :file %>
  </div>

  <div class="">
    <%= f.button :submit %>
  </div>
<% end %>
